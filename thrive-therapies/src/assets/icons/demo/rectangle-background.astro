---
import type { HTMLAttributes } from 'astro/types';

interface Props extends HTMLAttributes<'svg'> {
  color?: string;
  size?: number | string;
}

const { class: className, ...rest } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 128 128"
  width="1em"
  height="1em"
  class:list={[className]}
  {...rest}
>
  <rect y="6" width="36" height="36" rx="6" fill="#BCB2F8"></rect>
  <rect x="6" width="36" height="36" rx="6" fill="#301E53"></rect>
  <path
    d="M17.251 24.75H30.751"
    stroke="#865AD2"
    stroke-width="1.5"
    stroke-linecap="round"
    stroke-linejoin="round"></path>
  <path
    fill-rule="evenodd"
    clip-rule="evenodd"
    d="M17.251 9.75C17.251 8.09315 18.5941 6.75 20.251 6.75H27.751C29.4079 6.75 30.751 8.09315 30.751 9.75V26.25C30.751 27.9069 29.4079 29.25 27.751 29.25H20.251C18.5941 29.25 17.251 27.9069 17.251 26.25V9.75Z"
    stroke="white"
    stroke-width="1.5"
    stroke-linecap="round"
    stroke-linejoin="round"></path>
</svg>
